function inTheBeginning(what) {
	if (!(1 == 2) && -3 + 6*3 > 5*2 + 4 || false) {
		echo "In the beginning, there was " + what;
	}
}


inTheBeginning("." + "..?");

input what;

inTheBeginning(what + ("!" + "!"));

echo "Program finished. Press enter key to continue...";
input what;

